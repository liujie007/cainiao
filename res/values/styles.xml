<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.>
    -->
    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/room_rating_bar</item>
        <item name="android:minHeight">24dp</item>
        <item name="android:maxHeight">24dp</item>
        <item name="android:thumb">@null</item>
    </style>

    <style name="reg_edittext">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">22dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->

    </style>

    <style name="SplashTheme" parent="android:Theme.Black.NoTitleBar.Fullscreen">
        <item name="android:windowEnterAnimation">@anim/splash_fade_in</item>
        <item name="android:windowExitAnimation">@anim/splash_fade_out</item>
    </style>

    <style name="ButtomBar">
        <item name="android:layout_height">@dimen/main_bar_button_height</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="ButtomBarImgBtn">
        <item name="android:layout_height">@dimen/main_bar_button_height</item>
        <item name="android:layout_width">@dimen/main_bar_button_width</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="ButtomMainImgBtn">
        <item name="android:layout_height">@dimen/main_button_height</item>
        <item name="android:layout_width">@dimen/main_button_width</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <!-- 医生主页的Bar -->
    <style name="text_bar_style">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0.0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">#424344</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
        <item name="android:clickable">true</item>
    </style>
  <style name="text_nete_style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
      
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">#424344</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
        <item name="android:clickable">true</item>
    </style>
    <!-- 挂号信息 -->
    <style name="text_registration">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!-- 挂号信息中分割线 -->
    <style name="deliver_line_registration">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@drawable/invented_line</item>
    </style>

    <!-- 挂号信息 -->
    <style name="linear_registrtion">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">2.0dp</item>
    </style>
    <!-- 挂号信息 -->
    <style name="text_person_registrtion">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <!-- 挂号信息-体检人 -->
    <style name="text_person_registrtion_person">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/common_title_bar_background</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <style name="text_person_phyinfo">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/deliver_line</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <!-- 添加银行卡中的EditText -->
    <style name="edit_add_card">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColorHint">@color/deliver_line</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginLeft">10.0dp</item>
    </style>

    <!-- 添加银行卡中的Linearlayout -->
    <style name="linear_add_card">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:padding">5.0dp</item>
        <item name="android:layout_marginTop">10.0dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!-- 添加银行卡中的TextView -->
    <style name="text_add_card">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <!-- 提交、确认按钮 -->
    <style name="button_confirm">
        <item name="android:layout_height">45.0dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">30.0dp</item>
        <item name="android:background">@drawable/button_confirm</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <!-- RelativeLayout -->
    <style name="relativlayout">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@color/white</item>
        <item name="android:focusable">false</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <!-- Select -->
    <style name="select">
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">0.1dp</item>
        <item name="android:contentDescription">@string/app_name</item>
    </style>

    <style name="select_bar">
        <item name="android:layout_width">0.0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:background">@drawable/selector_phymenu_select</item>
        <item name="android:button">@null</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5.0dp</item>
        <item name="android:textColor">@drawable/textcolor</item>
    </style>

    <style name="popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>

    <style name="img_person_center">
        <item name="android:layout_width">30.0dp</item>
        <item name="android:layout_height">30.0dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">12.0dp</item>
    </style>

    <style name="img_arrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">22.0dp</item>
        <item name="android:src">@drawable/img_item_arrow</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="text_person_center">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">15.0dp</item>
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <style name="rl_persona_center">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/person_center</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/selector_bg_persona_rl</item>
